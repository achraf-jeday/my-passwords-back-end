{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/codesnippet": "^1.7",
        "drupal/consumers": "^1.11",
        "drupal/core": "^9.3",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/google_analytics": "^4.0",
        "drupal/hreflang": "^1.5",
        "drupal/jsonapi_extras": "^3.20",
        "drupal/memcache_storage": "^2.0",
        "drupal/metatag": "^1.16",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/rest_password": "^8.1",
        "drupal/restui": "^1.20",
        "drupal/schema_metatag": "^2.2",
        "drupal/seo_checklist": "^5.1",
        "drupal/simple_oauth": "^5.0",
        "drupal/simple_sitemap": "^4.0",
        "drupal/subrequests": "^3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "cweagans/composer-patches": true
        },
        "process-timeout": 2000
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "User created via /user/register?_format=json get blocked": "https://www.drupal.org/files/issues/2019-10-24/drupal-3055807-user_registered-via-rest-get-blocked-7.patch"
            },
            "drupal/rest_password": {
                "Possible email enumeration vulnerability": "https://www.drupal.org/files/issues/2021-12-12/rest_password-3223629-4.patch"
            }
        }
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "drupal/config_partial_export": "^1.6",
        "drupal/console": "~1.0",
        "drupal/devel": "^4.1",
        "drush/drush": "10.x",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "post-install-cmd": [
            "./scripts/symlink.sh"
        ]
    }
}
